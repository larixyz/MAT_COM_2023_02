class FuncoesLogicaProposicional:
    @staticmethod
    def implicacao(p, q):
        # Implicação: Retorna True se p implica em q, caso contrário, retorna False.
        return not p or q

# Exemplo de uso:
p = True
q = False

logica = FuncoesLogicaProposicional()

resultado_implicacao = logica.implicacao(p, q)

print(f'Implicação: p -> q = {resultado_implicacao}')
